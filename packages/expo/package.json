  "version": "1.0.0",
  "scripts": {
    "android": "expo run:android",
    "build:configure": "npx eas-cli@latest build:configure",
    "build:android:dev": "npx eas-cli@latest build --platform android --profile development",
    "build:android:prod": "npx eas-cli@latest build --platform android --profile production",
