    "eas:login": "npx eas-cli@latest login",
    "eas:submit:android": "npx eas-cli@latest submit --platform android",
    "eas:submit:ios": "npx eas-cli@latest submit --platform ios",
    "eas:whoami": "npx eas-cli@latest whoami",
    "format": "prettier . --write",
    "ios": "expo run:ios",
    "lint": "expo lint",
    "list-routes": "npx expo-router-sitemap",
    "prebuild": "expo prebuild --clean",
    "start": "expo start --clear",
    "test": "jest --watchAll"
  },
